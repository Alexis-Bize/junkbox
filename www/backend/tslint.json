{
	"extends": ["tslint:recommended", "tslint-config-prettier"],
	"rules": {
		"curly": false,
		"no-console": false,
		"interface-over-type-literal": false,
		"variable-name": false,
		"ordered-imports": false,
		"object-literal-sort-keys": false
	},
	"linterOptions": {
		"exclude": ["node_modules/**/*"]
	}
}
